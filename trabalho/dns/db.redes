$TTL 604800
@ IN SOA dns.redes. root.redes. (
  2 ; Serial
  604800 ; Refresh
  86400 ; Retry
  2419200 ; Expire
  604800 ) ; Negative Cache TTL
;
@       IN NS dns.redes.
dns     IN A 192.168.1.10
ftp     IN A 192.168.1.30
web     IN A 192.168.1.20
ldap    IN A 192.168.1.40
samba   IN A 192.168.1.50